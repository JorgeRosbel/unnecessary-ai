    "zod": "^3.25.0"
  },
  "dependencies": {
    "openai": "^5.10.1"
  },
  "devDependencies": {
    "@esbuild-plugins/tsconfig-paths": "^0.1.2",
    "@types/node": "^24.1.0",
    "@vitest/coverage-v8": "^3.2.4",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.2",
    "prettier": "^3.6.2",
    "tsup": "^8.5.0",
    "tsx": "^4.20.4",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4",
    "zod": "^3.25.76"
  }
}
