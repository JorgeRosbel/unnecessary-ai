{
  "name": "unnecessary-ai",
  "version": "0.0.1",
  "description": "",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "main": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsup",
    "prepare": "husky",
    "format": "prettier --write \"**/*.{js,ts}\"",
